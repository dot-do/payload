{
  "name": "@dotdo/db-clickhouse",
  "version": "0.1.6",
  "description": "ClickHouse database adapter for Payload CMS, optimized for Cloudflare Workers",
  "keywords": [
    "payload",
    "payloadcms",
    "clickhouse",
    "database",
    "adapter",
    "cloudflare",
    "workers",
    "edge"
  ],
  "homepage": "https://github.com/dot-do/payload/tree/main/packages/db-clickhouse",
  "repository": {
    "type": "git",
    "url": "https://github.com/dot-do/payload.git",
    "directory": "packages/db-clickhouse"
  },
  "license": "MIT",
  "author": "dot.do",
  "maintainers": [
    {
      "name": "dot.do",
      "url": "https://dot.do"
    }
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./local": {
      "import": "./dist/local.js",
      "types": "./dist/local.d.ts",
      "default": "./dist/local.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm build:types && pnpm build:swc",
    "build:debug": "pnpm build",
    "build:swc": "swc ./src -d ./dist --config-file .swcrc-build --strip-leading-paths",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "clean": "rimraf -g {dist,*.tsbuildinfo}",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@clickhouse/client-web": "^1.8.1",
    "ulid": "3.0.2"
  },
  "devDependencies": {
    "@payloadcms/eslint-config": "workspace:*",
    "chdb": "^1.3.0",
    "payload": "workspace:*",
    "vitest": "3.2.3"
  },
  "peerDependencies": {
    "chdb": "^1.0.0",
    "payload": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "chdb": {
      "optional": true
    }
  },
  "publishConfig": {
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "./local": {
        "import": "./dist/local.js",
        "types": "./dist/local.d.ts",
        "default": "./dist/local.js"
      }
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts"
  }
}
